/* profile.wxss */
.container {
  text-align: center;
  padding: 20px;
}

.profile-info {
  margin-bottom: 20px;
}

.avatar {
  width: 80px;
  height: 80px;
  border-radius: 50%;
  margin-bottom: 10px;
}

.username,
.email {
  font-size: 16px;
  margin-bottom: 5px;
}

.logout {
  color: #007aff;
  font-size: 16px;
  text-decoration: underline;
  cursor: pointer;
}
